<?xml version="1.0" encoding="UTF-8" ?>
<site id="site" defaultChildId="home">
	<title>@site.title@</title>
	
	<children>
		<home id="home" uriNode="home">
			<title>@home.title@</title>
			<imageUrl>@home.imageUrl@</imageUrl>
			<newsTitle>home.newsTitle</newsTitle>
			<newsBody>home.newsBody</newsBody>
			<featuredArtistTitle>home.featuredArtistTitle</featuredArtistTitle>
			<featuredArtistBody>home.featuredArtistBody</featuredArtistBody>
			<somethingTitle>home.somethingTitle</somethingTitle>
			<somethingBody>home.somethingBody</somethingBody>
		</home>
		<gallery id="gallery" uriNode="gallery" defaultChildId="artist1">
			<title>@gallery.title@</title>
			<children>
				<artist id="artist1" uriNode="jamiecopeland" defaultChildId="1">
					<title>@gallery.artist1.title@</title>
					<children>
						<image id="1" uriNode="1">
							<title>@gallery.artist1.image1.title@</title>
							<url>@gallery.artist1.image1.url@</url>
						</image>
						<image id="2" uriNode="2">
							<title>@gallery.artist1.image2.title@</title>
							<url>@gallery.artist1.image2.url@</url>
						</image>
						<image id="3" uriNode="3">
							<title>@gallery.artist1.image3.title@</title>
							<url>@gallery.artist1.image3.url@</url>
						</image>
					</children>
				</artist>
				<artist id="artist2" uriNode="mattsweetman" defaultChildId="1">
					<title>@gallery.artist2.title@</title>
					<children>
						<image id="1" uriNode="1">
							<title>@gallery.artist2.image1.title@</title>
							<url>@gallery.artist2.image1.url@</url>
						</image>
						<image id="2" uriNode="2">
							<title>@gallery.artist2.image2.title@</title>
							<url>@gallery.artist2.image2.url@</url>
						</image>
						<image id="3" uriNode="3">
							<title>@gallery.artist2.image3.title@</title>
							<url>@gallery.artist2.image3.url@</url>
						</image>
					</children>
				</artist>
			</children>
		</gallery>
		<contact id="contact" uriNode="contact">
			<title>@contact.title@</title>
			<imageUrl>@contact.imageUrl@</imageUrl>
			<address>@contact.address@</address>
			<email>@contact.email@</email>
		</contact>
	</children>
	
	<footer>
		<copyright>@footer.copyright@</copyright>
	</footer>
</site>